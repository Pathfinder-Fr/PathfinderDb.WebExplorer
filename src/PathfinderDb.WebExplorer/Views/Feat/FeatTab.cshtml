@model FeatIndexViewModel
<table class="table table-striped table-bordered table-hover table-condensed">
    <thead>
        <tr>
        <th>Nom</th>
            <th>Résumé</th>
            </tr>
    </thead>
    @foreach (var feat in this.Model.Feats)
    {
        <tr>
            <td>@feat.Name</td>
            <td>@feat.Description</td>
        </tr>
    }
</table>
